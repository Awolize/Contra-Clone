#include "PowerUp.h"
#include <iostream>

PowerUp::PowerUp(sf::Texture* texture, sf::Vector2f size, sf::Vector2f position, std::string name)
  : Entity(position), name{ name }
{
  body.setSize(size);
  body.setTexture(texture);
}
void PowerUp::Draw(sf::RenderWindow & window)
{
	window.draw(body);
}

